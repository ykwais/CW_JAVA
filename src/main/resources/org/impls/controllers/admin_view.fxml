<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="240.0" prefWidth="248.0" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.impls.controllers.AdminController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <!-- Неизменяемое текстовое поле -->
    <TextField fx:id="adminTextField" editable="false" maxWidth="300" text="Информация администратора" />

    <!-- Кнопка "Backup" -->
    <Button fx:id="backupButton" maxWidth="200" text="Backup" />
   <Button fx:id="rollBackButton" mnemonicParsing="false" text="RollBack" />

    <!-- Кнопка "Посмотреть информацию" -->
    <Button fx:id="viewInfoButton" maxWidth="200" text="Посмотреть информацию" />

    <!-- Кнопка "Посмотреть пользователей" -->
    <Button fx:id="viewUsersButton" maxWidth="200" text="Посмотреть пользователей" />

    <Button fx:id="backButton" maxWidth="200" text="Back" />
</VBox>
