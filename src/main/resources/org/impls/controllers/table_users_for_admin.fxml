<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.impls.controllers.TableUsersAdminController">
    <Button fx:id="backButton" mnemonicParsing="false" text="Back">
        <VBox.margin>
            <Insets bottom="20.0" />
        </VBox.margin>
    </Button>
    <TableView fx:id="tableView" prefHeight="304.0" prefWidth="900.0">
        <columns>
            <TableColumn fx:id="loginColumn" text="Login" />
            <TableColumn fx:id="emailColumn" prefWidth="180.0" text="Email" />
            <TableColumn fx:id="realNameColumn" prefWidth="180.0" text="Real Name" />
            <TableColumn fx:id="createdAtColumn" prefWidth="150.0" text="Created At" />
            <TableColumn fx:id="totalBookingsColumn" prefWidth="150.0" text="Total Bookings" />
            <TableColumn fx:id="actionColumn" prefWidth="100.0" text="Action" />
        </columns>
    </TableView>
</VBox>
