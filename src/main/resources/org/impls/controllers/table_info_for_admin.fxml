<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.impls.controllers.TableInfoAdminController">
    <Button fx:id="backButton" mnemonicParsing="false" text="Back">
        <VBox.margin>
            <Insets bottom="20.0" />
        </VBox.margin>
    </Button>
    <TableView fx:id="tableView" prefHeight="304.0" prefWidth="900.0">
        <columns>
            <TableColumn fx:id="loginColumn" text="Login" />
            <TableColumn fx:id="emailColumn" prefWidth="180.0" text="Email" />
            <TableColumn fx:id="realNameColumn" prefWidth="180.0" text="Real Name" />
            <TableColumn fx:id="brandColumn" prefWidth="100.0" text="Brand" />
            <TableColumn fx:id="modelColumn" prefWidth="100.0" text="Model" />
            <TableColumn fx:id="startDateColumn" prefWidth="120.0" text="Start Date" />
            <TableColumn fx:id="endDateColumn" prefWidth="120.0" text="End Date" />
            <TableColumn fx:id="priceColumn" prefWidth="100.0" text="Price/Day" />
        </columns>
    </TableView>
</VBox>
